{
  "name": "fastify-typescript-starter",
  "version": "0.1.0",
  "description": "Fastify TypeScript starter repository",
  "scripts": {
    "dev": "nodemon --watch './src/**/*.ts' --exec ts-node ./src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js"
  },
  "author": "Wahyufadz <wahyufadz@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "fastify": "^3.7.0",
    "fastify-formbody": "^5.0.0",
    "fastify-typeorm-plugin": "^2.1.3",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "eslint": "^7.12.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
